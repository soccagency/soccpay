SOCCPAY_BASE_URL=https://api.soccpay.com/

# SoccPay API Credentials
# Get these from your SoccPay merchant dashboard
# Go to Merchants -> Settings (gear icon)
SOCCPAY_CLIENT_ID=your_client_id_here
SOCCPAY_CLIENT_SECRET=your_client_secret_here

# Server Configuration (for examples)
PORT=3000
NODE_ENV=development

# Success and Cancel URLs (for examples)
# These should point to your application's callback endpoints
SUCCESS_URL=http://localhost:3000/payment/success
CANCEL_URL=http://localhost:3000/payment/cancel